<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>iBatis</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
  	<servlet-name>admin</servlet-name>
  	<servlet-class>org.blprr.proj.sms.maintenance.user.admin.AdminUser</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>admin</servlet-name>
  	<url-pattern>/admin</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>admin</servlet-name>
  	<url-pattern>/searchRecord</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>admin</servlet-name>
  	<url-pattern>/insertRecord</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>admin</servlet-name>
  	<url-pattern>/updateRecord</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>admin</servlet-name>
  	<url-pattern>/updateAdminLogDate</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>user</servlet-name>
  	<servlet-class>org.blprr.proj.sms.maintenance.user.user.UserUser</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>user</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>user</servlet-name>
  	<url-pattern>/updateProfile</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>user</servlet-name>
  	<url-pattern>/updatePassword</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>user</servlet-name>
  	<url-pattern>/updateUserLogDate</url-pattern>
  </servlet-mapping>
  
</web-app>